{
  "name": "unocss-preset-naive-ui",
  "version": "0.0.2",
  "description": "unocss + naive-ui",
  "keywords": [
    "unocss",
    "preset",
    "naive-ui"
  ],
  "homepage": "https://github.com/ModyQyW/unocss-preset-naive-ui",
  "bugs": {
    "url": "https://github.com/ModyQyW/unocss-preset-naive-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ModyQyW/unocss-preset-naive-ui.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://github.com/ModyQyW"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "check:deps": "taze",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "unbuild --stub",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\" \"pnpm:lint:publint\"",
    "lint:eslint": "eslint . --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:publint": "publint",
    "prepare": "is-ci || simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "release": "commit-and-tag-version -a",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.0.2",
    "unocss": "^0.58.3",
    "unocss-preset-theme": "^0.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@commitlint/prompt": "^18.4.4",
    "@modyqyw/fabric": "^9.0.6",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "commit-and-tag-version": "^12.1.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.0",
    "markdownlint-cli": "^0.38.0",
    "prettier": "^3.1.1",
    "publint": "^0.2.7",
    "simple-git-hooks": "^2.9.0",
    "sort-package-json": "^2.6.0",
    "taze": "^0.13.1",
    "type-fest": "^4.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.1.3"
  },
  "peerDependencies": {
    "unocss": ">=0.52",
    "unocss-preset-theme": ">=0.9"
  },
  "packageManager": "pnpm@8.14.0",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
